{
  "kind": "collectionType",
  "collectionName": "task_statuses",
  "info": {
    "singularName": "task-status",
    "pluralName": "task-statuses",
    "displayName": "Task Status"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "label": {
      "type": "string",
