    },
    "color": {
      "type": "string",
      "required": true
    },
    "sub_tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-task.sub-task",
      "mappedBy": "task_status"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task",
      "mappedBy": "task_status"
    }
  }
}
